{
  "name": "user-service",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "test": "jest",
    "watch": "jest --watch",
    "coverage": "jest --coverage",
    "prettier": "prettier .",
    "prettier:fix": "prettier --write \"**/*.*\""
  },
  "dependencies": {
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.13.4",
    "express-statsd": "0.3.0",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.0",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "jest": "26.0.1",
    "prettier": "2.0.5",
    "supertest": "4.0.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/models/*.{js,jsx}",
      "**/data/*.{js,jsx}",
      "**/routes/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/scripts/**"
    ]
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  }
}
